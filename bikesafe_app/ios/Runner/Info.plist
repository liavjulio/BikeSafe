	<key>GIDClientID</key>
	<string>$(GOOGLE_SIGN_IN_CLIENT_ID)</string>
	<key>io.flutter.embedded_views_preview</key>
	<true/>
	<key>CFBundleURLTypes</key>
	<array>
		<dict>
			<key>CFBundleURLSchemes</key>
			<array>
				<!-- Correct URL scheme -->
				<string>com.googleusercontent.apps.1087501086163-a5lj3dgfdhhs3mrjvf64613go9fg5134</string>
			</array>
		</dict>
	</array>
	<key>GMSApiKey</key>
	<string>$(GOOGLE_MAPS_API_KEY)</string>
</dict>
